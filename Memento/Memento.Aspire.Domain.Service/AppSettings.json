{
	"Database":
	{
		"ConnectionString": ""
	},
	"MessageBus":
	{
		"ConnectionString": ""
	},
	"Localizer":
	{
		"DefaultCulture": "EN",
		"SupportedCultures":
		[
			"EN",
			"PT"
		]
	},
	"Logging":
	{
		"HttpLogging":
		{
			"Fields": "All"
		}
	},
	"Serilog":
	{
		"MinimumLevel":
		{
			"Default": "Information",
			"Override":
			{
				"Microsoft.AspNetCore": "Warning",
				"Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
				"Microsoft.EntityFrameworkCore": "Warning"
			}
		},
		"Using":
		[
			"Serilog.Enrichers.ClientInfo",
			"Serilog.Sinks.Async",
			"Serilog.Sinks.Console",
			"Serilog.Sinks.Seq"
		],
		"Enrich":
		[
			"FromLogContext",
			"WithClientIp",
			"WithCorrelationId"
		],
		"WriteTo":
		[
			{
				"Name": "Async",
				"Args":
				{
					"configure":
					[
						{
							"Name": "Console",
							"Args":
							{
								"applyThemeToRedirectedOutput": true,
								"theme": "Memento.Aspire.Core.Logging.LoggingExtensions::ConsoleTheme, Memento.Aspire.Core",
								"outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u4}] {SourceContext}{NewLine}{@Message:j}{NewLine}{@Properties:j}{NewLine}{Exception}{NewLine}"
							}
						},
						{
							"Name": "Seq",
							"Args":
							{
								"serverUrl": ""
							}
						}
					]
				}
			}
		]
	}
}
